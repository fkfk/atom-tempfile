{
  "name": "tempfile",
  "main": "./lib/tempfile",
  "version": "0.1.2",
  "description": "Create temporary file. similar to 'open-junk-file.el'.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "tempfile:create",
      "tempfile:paste"
    ]
  },
  "configSchema": {
    "save": {
      "order": 1,
      "type": "boolean",
      "default": false,
      "description": "Save tempfile"
    },
    "directory": {
      "order": 2,
      "type": "string",
      "description": "Tempfile save directory(if save option enabled)"
    },
    "addExtension": {
      "order": 3,
      "type": "boolean",
      "default": true,
      "description": "Add file extension"
    },
    "autoIndent": {
      "order": 4,
      "type": "boolean",
      "default": false,
      "description": "Auto indent pasted text"
    }
  },
  "repository": "https://github.com/fkfk/atom-tempfile",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^2.8.1",
    "moment": "^2.11.0",
    "tmp": "^0.0.28",
    "underscore-plus": "^1.6.6"
  }
}
